{
  "expo": {
    "name": "diarypic-fe",
    "slug": "diarypic-fe",
    "scheme": [
      "diarypic.app",
      "com.apply.diarypic",
      "kakao4aea82ec6ceb08c4f6d7f0fb157c8ee1"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "entryPoint": "node_modules/expo-router/entry.js",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apply.diarypic",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "사진을 업로드하기 위해 앨범 접근 권한이 필요합니다.",
        "NSCameraUsageDescription": "사진 촬영을 위해 카메라 접근 권한이 필요합니다.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "android": {
          "permissions": [
            "READ_EXTERNAL_STORAGE",
            "WRITE_EXTERNAL_STORAGE"
          ]
        }
      },
      "package": "com.apply.diarypic",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "BACKEND_URL": "http://localhost:8080",
      "router": {},
      "eas": {
        "projectId": "98161b2c-c0b4-419d-ac4d-e76300ee8121"
      }
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "@react-native-seoul/kakao-login",
        {
          "kakaoAppKey": "ff26e5dd397aa1e1d9927f1e0531548d",
          "kotlinVersion": "1.9.0"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              "https://devrepo.kakao.com/content/groups/public/"
            ]
          }
        }
      ],
      "expo-font",
      "expo-secure-store"
    ]
  }
}
